        })();
      </script>
    <% end %>

    <!-- google places -->
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=<%= Rails.application.secrets.google_api_key %>&libraries=places"></script>
  </head>
  <body class="mdl-color-text--grey-800 mdl-base">
    <% if current_user %>
      <%= render "layouts/dashboard" %>
    <% else %>
      <%= render "layouts/not_authenticated" %>
    <% end %>
    <script defer src="https://code.getmdl.io/1.2.0/material.min.js"></script>
  </body>
</html>
